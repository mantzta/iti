{"ast":null,"code":"import axios from 'axios';\nimport { store } from '../store';\nimport SideMenu from '../components/SideMenu.vue';\nexport default {\n  name: 'GetWood',\n  components: {\n    SideMenu\n  },\n\n  data() {\n    return {\n      errors: [],\n      store,\n      postBody: {\n        v: true,\n        seg: [{\n          \"start\": 0,\n          \"stop\": 5,\n          \"bri\": 255\n        }]\n      }\n    };\n  },\n\n  methods: {\n    sendWood() {\n      console.log(\"sends wood now to fire\");\n      axios.post(\"http://\" + store.ip + \"/json/state\", {\n        body: this.postBody\n      }).then().catch(e => {\n        this.errors.push(e);\n      });\n    },\n\n    hideSuccess() {\n      this.store.triggerWood = false;\n    }\n\n  },\n\n  async created() {\n    if (store.triggerWood) {\n      this.sendWood();\n    }\n  }\n\n};","map":{"version":3,"mappings":"AAYA,OAAOA,KAAP,MAAkB,OAAlB;AACA,SAASC,KAAT,QAAsB,UAAtB;AACA,OAAOC,QAAP,MAAqB,4BAArB;AAEA,eAAe;AACbC,MAAI,EAAE,SADO;AAGbC,YAAU,EAAE;AACVF;AADU,GAHC;;AAObG,MAAI,GAAG;AACL,WAAO;AACLC,YAAM,EAAE,EADH;AAELL,WAFK;AAGLM,cAAQ,EAAE;AACRC,SAAC,EAAE,IADK;AAERC,WAAG,EAAE,CACD;AACE,mBAAS,CADX;AAEE,kBAAQ,CAFV;AAGE,iBAAO;AAHT,SADC;AAFG;AAHL,KAAP;AAcD,GAtBY;;AAwBbC,SAAO,EAAE;AACPC,YAAQ,GAAG;AACTC,aAAO,CAACC,GAAR,CAAY,wBAAZ;AACAb,WAAK,CAACc,IAAN,CAAW,YAAYb,KAAK,CAACc,EAAlB,GAAuB,aAAlC,EAAiD;AAC/CC,YAAI,EAAE,KAAKT;AADoC,OAAjD,EAGCU,IAHD,GAICC,KAJD,CAIOC,KAAK;AACV,aAAKb,MAAL,CAAYc,IAAZ,CAAiBD,CAAjB;AACD,OAND;AAOD,KAVM;;AAWPE,eAAW,GAAG;AACZ,WAAKpB,KAAL,CAAWqB,WAAX,GAAyB,KAAzB;AACD;;AAbM,GAxBI;;AAwCb,QAAMC,OAAN,GAAgB;AACd,QAAItB,KAAK,CAACqB,WAAV,EAAuB;AACrB,WAAKX,QAAL;AACF;AACF;;AA5Ca,CAAf","names":["axios","store","SideMenu","name","components","data","errors","postBody","v","seg","methods","sendWood","console","log","post","ip","body","then","catch","e","push","hideSuccess","triggerWood","created"],"sourceRoot":"","sources":["/Users/tamara/Docs/01_Master/Interaction Technology Innovation/Project/iti/src/components/GetWood.vue"],"sourcesContent":["<template>\n\n<side-menu :red=\"this.store.red\" :green=\"this.store.green\" :blue=\"this.store.blue\"></side-menu>\n\n<div class=\"success\" :class=\"{'visible':store.triggerWood, 'hidden':!store.triggerWood}\">\n  <h3>Sarah added some wood to your fire for the next 10 minutes</h3>\n  <button @click=\"hideSuccess\">Great!</button>\n</div>\n  \n</template>\n\n<script>\nimport axios from 'axios';\nimport { store } from '../store'\nimport SideMenu from '../components/SideMenu.vue'\n\nexport default {\n  name: 'GetWood',\n\n  components: {\n    SideMenu,\n  },\n\n  data() {\n    return {\n      errors: [],\n      store,\n      postBody: {\n        v: true,\n        seg: [\n            {\n              \"start\": 0,\n              \"stop\": 5,\n              \"bri\": 255,\n            },\n          ],\n      },\n    }\n  },\n\n  methods: {\n    sendWood() {\n      console.log(\"sends wood now to fire\")\n      axios.post(\"http://\" + store.ip + \"/json/state\", {\n        body: this.postBody,\n      })\n      .then()\n      .catch(e => {\n        this.errors.push(e)\n      })\n    },\n    hideSuccess() {\n      this.store.triggerWood = false\n    },\n  },\n\n  async created() {\n    if (store.triggerWood) {\n      this.sendWood()\n    }\n  }\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\ndiv {\n  margin-top: 30px;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}