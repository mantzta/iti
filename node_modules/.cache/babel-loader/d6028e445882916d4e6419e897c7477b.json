{"ast":null,"code":"import axios from 'axios';\nimport SideMenu from '../components/SideMenu.vue';\nimport GetWood from '../components/GetWood.vue';\nimport { store } from '../store';\nexport default {\n  name: 'StartFire',\n  components: {\n    SideMenu,\n    GetWood\n  },\n  props: {\n    red: Number,\n    green: Number,\n    blue: Number\n  },\n\n  data() {\n    return {\n      store,\n      errors: [],\n      redM: null,\n      greenM: null,\n      blueM: null\n    };\n  },\n\n  methods: {\n    toggleFire() {\n      console.log(\"toggle fire\");\n      this.store.on = !this.store.on;\n      axios.post(\"http://\" + store.ip + \"/json/state\", {\n        on: false,\n        v: true,\n        seg: [{\n          start: 0,\n          stop: 15,\n          bri: 50,\n          col: [[this.store.red, this.store.green, this.store.blue, 1], [0, 0, 0, 0], [0, 0, 0, 0]]\n        }]\n      }).then(response => {\n        console.log(this.store);\n        this.store.on = response.data.on;\n      }).catch(e => {\n        this.errors.push(e);\n      });\n    }\n\n  },\n\n  async created() {\n    try {\n      const response = await axios.get(\"http://\" + store.ip + \"/api//json/state\");\n      console.log(\"GOT A RESPONSE: \", response);\n      this.store.on = response.data.on;\n    } catch (e) {\n      this.errors.push(e);\n    }\n  }\n\n};","map":{"version":3,"mappings":"AA2CA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,QAAP,MAAqB,4BAArB;AACA,OAAOC,OAAP,MAAoB,2BAApB;AACA,SAASC,KAAT,QAAsB,UAAtB;AAEA,eAAe;AACbC,MAAI,EAAE,WADO;AAEbC,YAAU,EAAE;AACVJ,YADU;AAEVC;AAFU,GAFC;AAMbI,OAAK,EAAE;AACLC,OAAG,EAAEC,MADA;AAELC,SAAK,EAAED,MAFF;AAGLE,QAAI,EAAEF;AAHD,GANM;;AAYbG,MAAI,GAAG;AACL,WAAO;AACLR,WADK;AAELS,YAAM,EAAE,EAFH;AAGLC,UAAI,EAAE,IAHD;AAILC,YAAM,EAAE,IAJH;AAKLC,WAAK,EAAE;AALF,KAAP;AAOD,GApBY;;AAsBbC,SAAO,EAAE;AACPC,cAAU,GAAG;AACXC,aAAO,CAACC,GAAR,CAAY,aAAZ;AACA,WAAKhB,KAAL,CAAWiB,EAAX,GAAgB,CAAC,KAAKjB,KAAL,CAAWiB,EAA5B;AACApB,WAAK,CAACqB,IAAN,CAAW,YAAYlB,KAAK,CAACmB,EAAlB,GAAuB,aAAlC,EAAiD