{"ast":null,"code":"import { createRouter, createWebHistory } from 'vue-router';\nimport StartFire from './components/StartFire.vue';\nimport ChooseFeeling from './components/ChooseFeeling.vue';\nconst routes = [{\n  path: '/',\n  component: StartFire\n}, {\n  path: '/choose-feeling',\n  component: ChooseFeeling\n}];\nconst router = createRouter({\n  history: createWebHistory(process.env.BASE_URL),\n  routes\n});\nconst app = Vue.createApp({});\napp.use(router);\napp.mount('#app');\nexport default {\n  name: 'App',\n  components: {\n    StartFire\n  },\n\n  data() {\n    return {\n      showMenu: false\n    };\n  },\n\n  methods: {\n    openMenu() {\n      console.log(this.showMenu);\n      this.showMenu = true;\n    },\n\n    closeMenu() {\n      this.showMenu = false;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAkBA,SAASA,YAAT,EAAuBC,gBAAvB,QAA8C,YAA9C;AACA,OAAOC,SAAP,MAAsB,4BAAtB;AACA,OAAOC,aAAP,MAA0B,gCAA1B;AAEA,MAAMC,MAAK,GAAI,CACb;AAAEC,MAAI,EAAE,GAAR;AAAaC,WAAS,EAAEJ;AAAxB,CADa,EAEb;AAAEG,MAAI,EAAE,iBAAR;AAA2BC,WAAS,EAAEH;AAAtC,CAFa,CAAf;AAKA,MAAMI,MAAK,GAAIP,YAAY,CAAC;AAC1BQ,SAAO,EAAEP,gBAAgB,CAACQ,OAAO,CAACC,GAAR,CAAYC,QAAb,CADC;AAE1BP;AAF0B,CAAD,CAA3B;AAKA,MAAMQ,GAAE,GAAIC,GAAG,CAACC,SAAJ,CAAc,EAAd,CAAZ;AACAF,GAAG,CAACG,GAAJ,CAAQR,MAAR;AAEAK,GAAG,CAACI,KAAJ,CAAU,MAAV;AAEA,eAAe;AACbC,MAAI,EAAE,KADO;AAEbC,YAAU,EAAE;AACVhB;AADU,GAFC;;AAKbiB,MAAI,GAAG;AACL,WAAO;AACLC,cAAQ,EAAE;AADL,KAAP;AAGD,GATY;;AAUbC,SAAO,EAAE;AACPC,YAAQ,GAAG;AACTC,aAAO,CAACC,GAAR,CAAY,KAAKJ,QAAjB;AACA,WAAKA,QAAL,GAAgB,IAAhB;AACD,KAJM;;AAKPK,aAAS,GAAG;AACV,WAAKL,QAAL,GAAgB,KAAhB;AACF;;AAPO;AAVI,CAAf","names":["createRouter","createWebHistory","StartFire","ChooseFeeling","routes","path","component","router","history","process","env","BASE_URL","app","Vue","createApp","use","mount","name","components","data","showMenu","methods","openMenu","console","log","closeMenu"],"sourceRoot":"","sources":["/Users/tamara/Docs/01_Master/Interaction Technology Innovation/Project/iti/src/App.vue"],"sourcesContent":["<template>\n  <div :class=\"{ 'menu-open': showMenu, 'menu-closed': !showMenu }\">\n    <i :class=\"{ 'closebtn-visible': showMenu, 'closebtn-hidden': !showMenu }\" class=\"material-icons menu closebtn\" @click=\"closeMenu\">close</i>\n\n    <ul class=\"menu-content\">\n        <li><router-link to=\"/\">My fire</router-link></li>\n        <li><router-link to=\"/friends\">Friends' fires</router-link></li>\n        <li><router-link to=\"/settings\">Fire settings</router-link></li>\n    </ul>\n  </div>\n  <i class=\"material-icons menu\" @click=\"openMenu\">menu</i>\n  <div class=\"content\">\n    <StartFire/>\n    <router-view></router-view>\n  </div>\n</template>\n\n<script>\nimport { createRouter, createWebHistory} from 'vue-router'\nimport StartFire from './components/StartFire.vue'\nimport ChooseFeeling from './components/ChooseFeeling.vue'\n\nconst routes = [\n  { path: '/', component: StartFire },\n  { path: '/choose-feeling', component: ChooseFeeling },\n]\n\nconst router = createRouter({\n  history: createWebHistory(process.env.BASE_URL),\n  routes,\n})\n\nconst app = Vue.createApp({})\napp.use(router)\n\napp.mount('#app')\n\nexport default {\n  name: 'App',\n  components: {\n    StartFire\n  },\n  data() {\n    return {\n      showMenu: false\n    };\n  },\n  methods: {\n    openMenu() {\n      console.log(this.showMenu)\n      this.showMenu = true;\n    },\n    closeMenu() {\n      this.showMenu = false;\n    }\n  }\n}\n</script>\n\n<style>\n#app {\n  padding: 20px;\n}\n\n.content {\n  text-align: center;\n}\n\n.menu {\n  cursor: pointer;\n}\n\n.menu-open {\n  top: 0;\n  left: 0;\n  width: 50vw;\n  height: 100vh;\n  position: fixed;\n  background-color: #E8505B;\n  transition: 0.5s;\n  padding: 20px;\n}\n\n.menu-closed {\n  top: 0;\n  left: 0;\n  width: 0;\n  height: 100vh;\n  position: fixed;\n  overflow-y: hidden;\n  transition: 0.5s;\n  padding: 20px;\n}\n\n.material-icons.closebtn {\n  color: #ffffff;\n}\n\n.material-icons.closebtn-visible {\n  opacity: 1;\n  transition: 0.5s;\n}\n\n.material-icons.closebtn-hidden {\n  opacity: 0;\n  transition: 0.5s;\n}\n\n.menu-content {\n  color: #ffffff;\n}\n\nul {\n  list-style-type: none;\n  padding: 0;\n}\nli {\n  margin: 0 10px;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}