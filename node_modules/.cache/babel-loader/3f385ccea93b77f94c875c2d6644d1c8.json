{"ast":null,"code":"import axios from 'axios';\nimport { store } from '../store';\nexport default {\n  name: 'GetWood',\n\n  data() {\n    return {\n      errors: [],\n      store,\n      postBody: {\n        v: true,\n        seg: [{\n          \"start\": 0,\n          \"stop\": 5,\n          \"bri\": 255\n        }]\n      },\n      show: false\n    };\n  },\n\n  methods: {\n    sendWood() {\n      console.log(\"sends wood now to fire\");\n      axios.post(\"http://\" + store.ip + \"/json/state\", {\n        body: this.postBody\n      }).then().catch(e => {\n        this.errors.push(e);\n      });\n    },\n\n    hideSuccess() {\n      this.store.triggerWood = false;\n      this.show = false;\n    }\n\n  },\n\n  async created() {\n    if (store.triggerWood) {\n      setTimeout(() => {\n        this.sendWood();\n        this.show = true;\n      }, 20000);\n    }\n  }\n\n};","map":{"version":3,"mappings":"AAUA,OAAOA,KAAP,MAAkB,OAAlB;AACA,SAASC,KAAT,QAAsB,UAAtB;AAEA,eAAe;AACbC,MAAI,EAAE,SADO;;AAGbC,MAAI,GAAG;AACL,WAAO;AACLC,YAAM,EAAE,EADH;AAELH,WAFK;AAGLI,cAAQ,EAAE;AACRC,SAAC,EAAE,IADK;AAERC,WAAG,EAAE,CACD;AACE,mBAAS,CADX;AAEE,kBAAQ,CAFV;AAGE,iBAAO;AAHT,SADC;AAFG,OAHL;AAaLC,UAAI,EAAE;AAbD,KAAP;AAeD,GAnBY;;AAqBbC,SAAO,EAAE;AACPC,YAAQ,GAAG;AACTC,aAAO,CAACC,GAAR,CAAY,wBAAZ;AACAZ,WAAK,CAACa,IAAN,CAAW,YAAYZ,KAAK,CAACa,EAAlB,GAAuB,aAAlC,EAAiD;AAC/CC,YAAI,EAAE,KAAKV;AADoC,OAAjD,EAGCW,IAHD,GAICC,KAJD,CAIOC,KAAK;AACV,aAAKd,MAAL,CAAYe,IAAZ,CAAiBD,CAAjB;AACD,OAND;AAOD,KAVM;;AAWPE,eAAW,GAAG;AACZ,WAAKnB,KAAL,CAAWoB,WAAX,GAAyB,KAAzB;AACA,WAAKb,IAAL,GAAY,KAAZ;AACD;;AAdM,GArBI;;AAsCb,QAAMc,OAAN,GAAgB;AACd,QAAIrB,KAAK,CAACoB,WAAV,EAAuB;AACrBE,gBAAU,CAAC,MAAM;AACf,aAAKb,QAAL;AAEA,aAAKF,IAAL,GAAY,IAAZ;AACD,OAJS,EAIP,KAJO,CAAV;AAKF;AACF;;AA9Ca,CAAf","names":["axios","store","name","data","errors","postBody","v","seg","show","methods","sendWood","console","log","post","ip","body","then","catch","e","push","hideSuccess","triggerWood","created","setTimeout"],"sourceRoot":"","sources":["/Users/tamara/Docs/01_Master/Interaction Technology Innovation/Project/iti/src/components/GetWood.vue"],"sourcesContent":["<template>\n\n<div class=\"success\" :class=\"{'visible':show, 'hidden':!show}\">\n  <h3>Sarah added some wood to your fire for the next 10 minutes</h3>\n  <button @click=\"hideSuccess\">Great!</button>\n</div>\n  \n</template>\n\n<script>\nimport axios from 'axios';\nimport { store } from '../store'\n\nexport default {\n  name: 'GetWood',\n\n  data() {\n    return {\n      errors: [],\n      store,\n      postBody: {\n        v: true,\n        seg: [\n            {\n              \"start\": 0,\n              \"stop\": 5,\n              \"bri\": 255,\n            },\n          ],\n      },\n      show: false,\n    }\n  },\n\n  methods: {\n    sendWood() {\n      console.log(\"sends wood now to fire\")\n      axios.post(\"http://\" + store.ip + \"/json/state\", {\n        body: this.postBody,\n      })\n      .then()\n      .catch(e => {\n        this.errors.push(e)\n      })\n    },\n    hideSuccess() {\n      this.store.triggerWood = false\n      this.show = false\n    },\n  },\n\n  async created() {\n    if (store.triggerWood) {\n      setTimeout(() => {\n        this.sendWood()\n\n        this.show = true\n      }, 20000)\n    }\n  }\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\ndiv {\n  margin-top: 30px;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}