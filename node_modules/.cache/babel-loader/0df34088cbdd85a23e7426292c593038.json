         },\n          ],\n      },\n      show: false,\n    }\n  },\n\n  methods: {\n    sendWood() {\n      console.log(\"sends wood now to fire\")\n      axios.post(\"http://\" + store.ip + \"/json/state\", this.postBody)\n      .then(() => {\n        setTimeout(() => {\n          const postBody = {\n            v: true,\n            seg: [\n              {\n                start: 0,\n                stop: 15,\n                bri: 50,\n                col: [\n                  [this.store.red, this.store.green, this.store.blue, 1],\n                  [0, 0, 0, 0],\n                  [0, 0, 0, 0]\n                ],\n              },\n        ],\n          }\n          axios.post(\"http://\" + store.ip + \"/json/state\", postBody)\n        }, 60000)\n      })\n      .catch(e => {\n        this.errors.push(e)\n      })\n    },\n    hideSuccess() {\n      this.store.triggerWood = false\n      this.show = false\n    },\n  },\n\n  async created() {\n    if (store.triggerWood) {\n      setTimeout(() => {\n        this.sendWood()\n\n        this.show = true\n      }, 15000)\n    }\n  }\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\ndiv {\n  margin-top: 30px;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}